sub menu
kint=0
glcdmenu1_ili9326(50,200,ili9326_blue) 'print menu screen
'now print movable *
k9=4
kk1=1
do
if portb.6=1 then k9=k9+1:kk1=1
if portb.7=1 then k9=k9-1:kk1=1
if kk1=1 then
if k9>100 then k9=8 'gone negative?
if k9=0 then k9=8
if k9>8 then k9=1
GLCDChar_ili9326(20,y_ili9326,63,ili9326_black)
y_ili9326=220-k9*20
GLCDChar_ili9326(20,y_ili9326,63,ili9326_red)
wait 250 ms
kk1=0
end if
'kk1=0 'is this needed?
if kint=1 then exit do 'interrupt?

loop
kint=0
end sub

sub SendCommand_ili9326(ILI9326SendWord)
          set ILI9326_GLCD_CS off
          set ILI9326_GLCD_RS OFF
          set ILI9326_GLCD_RD ON
          set ILI9326_GLCD_WR ON
ILI9326_DataPort=ILI9326SendWord_h
          set ILI9326_GLCD_WR OFF
          set ILI9326_GLCD_WR ON
ILI9326_DataPort=ILI9326SendWord
          set ILI9326_GLCD_WR OFF
          set ILI9326_GLCD_WR ON
end sub

sub SendData_ILI9326(ILI9326SendWord)
          set ILI9326_GLCD_RS ON
ILI9326_DataPort=ILI9326SendWord_h
          set ILI9326_GLCD_WR OFF
          set ILI9326_GLCD_WR ON
ILI9326_DataPort=ILI9326SendWord
          set ILI9326_GLCD_WR OFF
          set ILI9326_GLCD_WR ON
end sub

sub GLCDCLS_ILI9326
SendCommand_ili9326(0x0202)
repeat 48000
sendData_ILI9326(ILI9326_black)
sendData_ILI9326(ILI9326_black)
end repeat
end sub
sub GLCDpixel_ili9326(x_ili9326,y_ili9326,col_ili9326)
'inverted landscape
sendCommand_ili9326(0x0201)
sendData_ili9326(400-x_ili9326)
sendCommand_ili9326(0x0200)
sendData_ili9326(y_ili9326)
sendCommand_ili9326(0x0202)
sendData_ili9326(0xFFFF)
end sub

sub glcdmenu1_ili9326(x_ili9326,y_ili9326,col_ili9326)
readtable menu1,0,xlen 'put chars in menu1 on screen
x1_ili9326=x_ili9326
for ii=1 to xlen
readtable menu1,ii,char_ili9326
if char_ili9326 = 0 then
y_ili9326=y_ili9326-20 'line feed
x_ili9326=x1_ili9326 'return
end if
if char_ili9326 >0 then
glcdchar_ili9326(x_ili9326,y_ili9326,char_ili9326,col_ili9326)
x_ili9326=x_ili9326+13
end if
next ii
end sub

sub GLCDline_ili9326(x_ili9326,ys_ili9326,ye_ili9326,col_ili9326)
start_ili9326=ys_ili9326

if ys_ili9326>=ye_ili9326 then
start_ili9326=ye_ili9326
length_ili9326=ys_ili9326-ye_ili9326
end if
if ys_ili9326<ye_ili9326 then
start_ili9326=ys_ili9326
length_ili9326=ye_ili9326-ys_ili9326
end if
'inverted landscape
sendCommand_ili9326(0x0201)
sendData_ili9326(400-x_ili9326)
sendCommand_ili9326(0x0200)
sendData_ili9326(start_ili9326)
sendCommand_ili9326(0x0202)
repeat length_ili9326+1
sendData_ili9326(col_ili9326)
end repeat
end sub

sub GLCDChar_ili9326(x_ili9326,y_ili9326,char_ili9326,col_ili9326)

for i=1 to 13
sendCommand_ili9326(0x0200)
sendData_ili9326(y_ili9326)
sendCommand_ili9326(0x0201)
sendData_ili9326(400-x_ili9326+i)
sendCommand_ili9326(0x0202)
readtable courier14,char_ili9326*13+i,xx
for j=0 to 15
col=ili9326_black
if xx.0 = 1 then col=col_ILI9326
sendData_ili9326(COL)
rotate xx right
next j
next i
end sub
sub GLCDStripes_ILI9326
'test for horizontal/vertical alignment
xx1=0x0000
SendCommand_ili9326(0x0202)
repeat GLCD_width
repeat GLCD_height
sendData_ILI9326(xx1)
end repeat
xx1=xx1+123
end repeat
end sub

sub wakeup
intoff
kint=kint+1 'flag for interrupt
if kint>1 then kint=0
'glcdprint(200,200,"I",ili9340_white)
wait 250 ms
inton
return
end sub






sub spectrum_run
xdelay=1
'dummy run to clear
set portc.5 off 'lamp on
set portc.7 off 'diode array on
wait 100 ms
for i=1 to 256
spectruml(i)=0
spectrumh(i)=0
spectrumv(i)=0
next i
'do
'k=5
for k=0 to 16
set portb.5 on 'shutter closed
set portb.4 on 'si high
wait xdelay us
set portb.2 on 'clk high
wait xdelay us
set portb.4 off 'si low
wait xdelay us
for i=1 to 256
if i=245 then set portb.5 off 'shutter open
set portb.2 on
datatemp=[word]0
datatemp=spectruml(i)
datatemp_h=spectrumh(i)
datatemp=datatemp+readad12(an9)
if k=0 then datatemp=[word]0 'dummy run
set portb.2 off
spectruml(i)=datatemp
spectrumh(i)=datatemp_h
wait xdelay us
next i
next k
'loop
set portc.5 on 'lamp off
set portc.7 on 'diode array off
for i=1 to 256
vr=spectruml(i)
vr_h=spectrumh(i)
calcabs
'spectruml(i)=abk
'spectrumh(i)=abk_h
next i
end sub


sub blank_run
xdelay=1
'dummy run to clear
set portc.5 off 'lamp on
set portc.7 off 'diode array on
wait 100 ms
for i=1 to 256
blankl(i)=0
blankh(i)=0
blankv(i)=0
next i
'do
for k=0 to 16
set portb.5 on 'shutter closed
set portb.4 on 'si high
wait xdelay us
set portb.2 on 'clk high
wait xdelay us
set portb.4 off 'si low
wait xdelay us
for i=1 to 256
if i=245 then set portb.5 off 'shutter open
set portb.2 on
datatemp=[word]0
datatemp=blankl(i)
datatemp_h=blankh(i)
datatemp=datatemp+readad12(an9)
if k=0 then datatemp=[word]0 'dummy run
set portb.2 off
blankl(i)=datatemp
blankh(i)=datatemp_h
wait xdelay us
next i
next k
'loop
set portc.5 on 'lamp off
set portc.7 on 'diode array off
end sub

sub calcabs
q1=61400
FOR I = 1 TO 300
readtable absorb,i,xx
q2=xx
IF VR > xx THEN goto qqf
Q1 = xx
NEXT I
abk=30000
return
qqf:
ABK = (q1 - q2)
q1 = ((VR - Q2)*10 / ABK)*10
q2=((vr-q2)*10)%abk
q1=q1+(q2*10)/abk
ABk = [word]I * 100 -q1 -100
if i>298 then abk=30000

end sub

sub flashdata
a1=a1+1
y1=x60
'a1=address y1=data
eeadrl=a1
eeadrh=a1_h
set EECON1.eepgd on ' Point to flash memory
set EECON1.cfgs off  ' Not configuration space
set EECON1.free off 'write
set EECON1.wren on  ' Enable writes
set EECON1.lwlo on  ' Write Latches
eedatl=y1
eedath=y1_h
set eecon1.lwlo off 'write
eecon2=0x55
eecon2=0xAA
set eecon1.wr on
nop
nop
set eecon1.wren off
end sub
